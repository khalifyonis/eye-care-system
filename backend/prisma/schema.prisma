generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id       String @id @default(uuid())
  fullName String
  username String @unique
  password String
  role     Role
}

enum Role {
  ADMIN
  DOCTOR
  PHARMACIST
  OPTICIAN
}
